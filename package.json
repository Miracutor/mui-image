{
  "name": "mui-image-alter",
  "version": "2.0.4",
  "description": "Display images as per the Material guidelines. For React apps using Material-UI v5.",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./ts": {
      "import": {
        "types": "./dist/ts.d.ts",
        "default": "./dist/ts.js"
      },
      "require": {
        "types": "./dist/ts.d.cts",
        "default": "./dist/ts.cjs"
      }
    }
  },
  "files": [
    "dist",
    "ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build:demo": "vite --config vite.democonfig.ts build",
    "preview": "vite preview",
    "size": "size-limit",
    "format": "prettier {demo,src}/**/*.{js,jsx,ts,tsx,json} --write",
    "clean": "rimraf dist jest-setup.d.ts mui-image-alter-*",
    "prepare": "npm run build",
    "preversion": "npm run clean && npm run format",
    "postversion": "git push && git push --tags",
    "prebuild": "npm run clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.1",
    "react": "^17.0.2 || ^18.0.0",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@fontsource/fira-code": "^5.0.15",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "size-limit": "^11.0.0",
    "tsup": "^8.0.1",
    "typeit-react": "^2.6.4",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "size-limit": [
    {
      "name": "cjs",
      "path": "dist/index.cjs",
      "gzip": true
    },
    {
      "name": "esm",
      "path": "dist/index.js",
      "gzip": true
    }
  ],
  "author": "mddanish00",
  "contributors": [
    "benmneb"
  ],
  "homepage": "https://mddanish00.github.io/mui-image",
  "license": "ISC",
  "repository": "https://github.com/mddanish00/mui-image",
  "keywords": [
    "material-ui",
    "material-ui image",
    "material-ui-image",
    "material design",
    "react",
    "loading image"
  ],
  "dependencies": {
    "clsx": "^2.0.0"
  }
}