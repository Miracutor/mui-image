{
	"name": "mui-image",
	"version": "1.0.8-beta.3",
	"description": "Display images as per the Material guidelines. For React apps using Material-UI v5.",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"es",
		"lib",
		"umd"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"demo-build": "vite --config vite.democonfig.ts build",
		"preview": "vite preview",
		"size": "size-limit",
		"clean": "rimraf dist es lib umd",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"peerDependencies": {
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@mui/material": "^5.0.1",
		"react": "^17.0.2 || ^18.0.0"
	},
	"devDependencies": {
		"@fontsource/fira-code": "^4.5.12",
		"@size-limit/preset-small-lib": "^7.0.8",
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"@vitejs/plugin-react": "^2.2.0",
		"rimraf": "^3.0.2",
		"size-limit": "^7.0.8",
		"typeit-react": "^2.6.0",
		"typescript": "^4.8.4",
		"vite": "3.1.8",
		"vite-plugin-build": "^0.6.0"
	},
	"size-limit": [
		{
			"path": "es/index.js"
		}
	],
	"author": "Miracutor",
	"homepage": "https://miracutor.github.io/mui-image",
	"license": "ISC",
	"repository": "https://github.com/Miracutor/mui-image",
	"keywords": [
		"material-ui",
		"material-ui image",
		"material-ui-image",
		"material design",
		"react",
		"loading image"
	],
	"dependencies": {
		"clsx": "^1.2.1"
	}
